{
  "name": "arkhamjs",
  "version": "3.5.0",
  "description": "Javascript Flux Library",
  "license": "MIT",
  "main": "index.js",
  "types": "./lib/index.d.ts",
  "typings": "lib/index",
  "keywords": [
    "arkhamjs",
    "flux",
    "global state",
    "immutable",
    "nitrogenlabs",
    "state",
    "state management"
  ],
  "author": {
    "name": "Giraldo Rosales",
    "email": "giraldo@nitrogenlabs.com",
    "url": "http://nitrogenlabs.com"
  },
  "repository": {
    "type": "git",
    "url": "nitrogenlabs/arkhamjs.git"
  },
  "homepage": "https://arkhamjs.io",
  "bugs": {
    "url": "nitrogenlabs/arkhamjs/issues"
  },
  "scripts": {
    "compile": "lex compile -r",
    "deploy": "lex build -r && s3-deploy './lib/static/**' --cwd './lib/static/' --region us-east-1 --bucket arkhamjs.io/versions --gzip",
    "lint": "eslint ./src --ext .ts,.tsx",
    "prepublishOnly": "yarn test && yarn compile",
    "pretest": "yarn lint",
    "reset": "rm -rf node_modules yarn.lock && yarn",
    "test": "lex test",
    "update": "lex update -i"
  },
  "dependencies": {
    "error-stack-parser": "^2.0.2",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "@nlabs/arkhamjs-storage-browser": "^1.0.5",
    "@types/jest": "^23.0.0",
    "@types/lodash": "^4.14.106",
    "@types/node": "^10.3.2",
    "eslint": "^4.19.1",
    "eslint-config-styleguidejs": "^0.7.4",
    "s3-deploy": "^1.0.0",
    "typescript": "^2.9.1"
  }
